#if(count(acronyms) > 0) {
  <table>
    <thead>
      <tr>
        <th>Short</th>
        <th>Long</th>
      </tr>
    </thead>
    <tbody>
      #for(acronym in acronyms) {
        <tr>
          <td>
            <a href="/acronyms/#(acronym.id)">
              #(acronym.short)
            </a>
          </td>
          <td>#(acronym.long)</td>
        </tr>
      }
    </tbody>
  </table>
} else {
  <h2>There arenâ€™t any acronyms yet!</h2>
}
